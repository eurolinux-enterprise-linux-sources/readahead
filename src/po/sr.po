msgid ""
"\n"
"%s [options] <file> [...]\n"
"\n"
msgstr ""
"\n"
"%s [опције] <датотека> [...]\n"
"\n"

#: src/readahead.c:425
msgid ""
"  -s | --sort            sort list of files only\n"
"  -o | --output <file>   output sorted list of files\n"
"  -d | --dont-sort       call readahead(2) for already sorted list\n"
"  -h | --help            this help\n"
"  -v | --verbose         verbose mode\n"
"  -t | --timing          report wasted time\n"
"  -m | --maxsize <10240> max size of a file to be preloaded (in KiB)\n"
"\n"
msgstr ""
"  -s | --sort            сортирај само листу датотека\n"
"  -o | --output <file>   излаз сортиране листе датотека\n"
"  -d | --dont-sort       позови readahead(2) за већ сортиране листе\n"
"  -h | --help            ова помоћ\n"
"  -v | --verbose         режим детаљног исписивања\n"
"  -t | --timing          извести о изгубљеном времену\n"
"  -m | --maxsize <10240> макс. величина датотеке за предучитавање (у КиБ)\n"
"\n"

